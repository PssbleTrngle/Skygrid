               </block>
            </side>
            <side on="west" shared="true">
               <block id="light">
                  <set key="level" value="0"/>
               </block>
            </side>
            <side on="east" shared="true">
               <block id="light">
                  <set key="level" value="0"/>
               </block>
            </side>
            <side on="up" probability="0.3">
               <block id="lily_pad"/>
            </side>
         </block>
      </list>
      <list weight="0.05" name="loot">

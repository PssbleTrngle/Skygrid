<preset>
   <list>
      <tag id="coral_blocks" mod="minecraft">
         <side on="up">
            <tag id="wall_corals" mod="minecraft"/>
            <tag id="coral_plants" mod="minecraft">
               <except/>
            </tag>
         </side>
      </tag>
      <block id="sand" mod="minecraft" weight="0.05">
         <side on="up">
            <block id="turtle_egg" mod="minecraft">
               <cycle key="eggs"/>
            </block>
         </side>
      </block>
      <list>
         <block id="sand" mod="minecraft"/>
         <block id="gravel" mod="minecraft"/>
         <side on="up">
            <block id="kelp" mod="minecraft"/>
            <block id="seagrass" mod="minecraft"/>
            <block id="tall_seagrass" mod="minecraft">
               <side on="up">
                  <block id="tall_seagrass" mod="minecraft">
                     <set key="half" value="upper"/>
                  </block>
               </side>
               <set key="half" value="lower"/>
            </block>
            <block id="sea_pickle" mod="minecraft">
               <cycle key="pickles"/>
            </block>
         </side>
      </list>
      <list name="prismarine">
         <list>
            <block id="prismarine" mod="minecraft"/>
            <block id="prismarine_bricks" mod="minecraft"/>
            <block id="dark_prismarine" mod="minecraft"/>
            <block id="sea_lantern" mod="minecraft"/>
         </list>
         <list weight="0.1" name="elder prismarine">
            <block id="elder_prismarine" mod="quark"/>
            <block id="elder_prismarine_bricks" mod="quark"/>
            <block id="elder_sea_lantern" mod="quark"/>
            <block id="dark_elder_prismarine" mod="quark"/>
         </list>
      </list>
   </list>
</preset>
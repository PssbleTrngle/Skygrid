<preset>
   <list>
      <tag id="coral_blocks">
         <side on="up">
            <tag id="coral_plants">
               <except>
                  <tag id="wall_corals"/>
               </except>
            </tag>
         </side>
      </tag>
      <block id="sand" weight="0.05">
         <side on="up">
            <block id="turtle_egg">
               <cycle key="eggs"/>
            </block>
         </side>
      </block>
      <list>
         <side on="up" shared="true">
            <block id="tall_seagrass">
               <set key="half" value="lower"/>
            </block>
         </side>
         <side on="up" offset="2" shared="true">
            <block id="tall_seagrass">
               <set key="half" value="upper"/>
            </block>
         </side>
         <side on="up">
            <block id="kelp"/>
            <block id="seagrass"/>
            <block id="sea_pickle">
               <cycle key="pickles"/>
            </block>
         </side>
         <block id="sand"/>
         <block id="gravel"/>
      </list>
      <list name="prismarine">
         <list>
            <block id="prismarine"/>
            <block id="prismarine_bricks"/>
            <block id="dark_prismarine"/>
            <block id="sea_lantern"/>
         </list>
         <list weight="0.1" name="elder prismarine">
            <block id="elder_prismarine" mod="quark"/>
            <block id="elder_prismarine_bricks" mod="quark"/>
            <block id="elder_sea_lantern" mod="quark"/>
            <block id="dark_elder_prismarine" mod="quark"/>
         </list>
      </list>
   </list>
</preset>